<!DOCTYPE html>
<html>
<head>
  <title>PmsApp</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= render 'layouts/stylesheets' %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
<%= render 'layouts/header' %>

<% flash.each do |key, value| %>
	<%= content_tag(:div, value, :class => "flash #{key}") %>
<% end %>
<%= yield %>
</div>
</body>
</html>
